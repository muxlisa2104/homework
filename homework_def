#1-topshiriq
def kopaytirish(*sonlar):
    kopaytma = 1
    for son in sonlar:
        kopaytma *= son
    return kopaytma
print(kopaytirish(9,7,11,24))


#2-topshiriq
def talabalar(ism, familiya, **malumotlar):
     talaba = {
         'Ism': ism,
         'Familiya': familiya
     }
     talaba.update(malumotlar)
     return talaba
talaba1 = talabalar('Muxlisa', 'Mavlanova', yosh=16, fakultet='Informatika')
talaba2 = talabalar('Quvonchbek', 'Mavlanov', yosh=21, kurs=3)
print(talaba1)
print(talaba2)
